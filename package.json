{
  "name": "css-to-js",
  "displayName": "css-to-js",
  "publisher": "soradev4",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:css-to-js.cssToJs"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "css-to-js.cssToJs",
        "title": "CSS to JS"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceLangId == css",
          "command": "css-to-js.cssToJs"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.45.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.30.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "glob": "^7.1.6",
    "mocha": "^10.2.0",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "ansi-styles": "^3.2.1",
    "chalk": "^2.4.2",
    "escape-string-regexp": "^1.0.5",
    "postcss": "^7.0.32",
    "source-map": "^0.6.1",
    "supports-color": "^6.1.0"
  }
}
